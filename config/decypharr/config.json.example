{
  "bind_address": "0.0.0.0",
  "url_base": "/",
  "port": "8282",
  "log_level": "debug",
  "debrids": [
    {
      "provider": "realdebrid",
      "name": "realdebrid",
      "api_key": "YOUR_REALDEBRID_API_KEY",
      "download_api_keys": [
        "YOUR_REALDEBRID_API_KEY"
      ],
      "rate_limit": "250/minute",
      "minimum_free_slot": 1,
      "torrents_refresh_interval": "10m",
      "download_links_refresh_interval": "5m",
      "workers": 300,
      "auto_expire_links_after": "3d"
    },
    {
      "provider": "torbox",
      "name": "torbox",
      "api_key": "YOUR_TORBOX_API_KEY",
      "download_api_keys": [
        "YOUR_TORBOX_API_KEY"
      ],
      "rate_limit": "250/minute",
      "minimum_free_slot": 1,
      "torrents_refresh_interval": "10m",
      "download_links_refresh_interval": "40m",
      "workers": 300,
      "auto_expire_links_after": "3d"
    }
  ],
  "arrs": [
    {
      "name": "radarr",
      "host": "http://radarr:7878",
      "token": "YOUR_RADARR_API_KEY",
      "download_uncached": false,
      "source": "auto"
    }
  ],
  "repair": {
    "enabled": true,
    "interval": "1h",
    "workers": 5,
    "strategy": "per_torrent"
  },
  "usenet": {
    "max_connections": 15,
    "read_ahead": "16MB",
    "processing_timeout": "10m",
    "availability_sample_percent": 10,
    "max_concurrent_nzb": 2,
    "disk_buffer_path": "/app/usenet/streams"
  },
  "mount": {
    "type": "dfs",
    "mount_path": "/mnt/decypharr",
    "rclone": {
      "port": "5572",
      "vfs_cache_mode": "off",
      "vfs_cache_max_age": "1h",
      "vfs_cache_poll_interval": "1m",
      "vfs_read_chunk_size_limit": "off",
      "async_read": false,
      "attr_timeout": "1s",
      "dir_cache_time": "5m",
      "log_level": "INFO"
    },
    "dfs": {
      "cache_expiry": "24h",
      "cache_dir": "/app/cache",
      "disk_cache_size": "2GB",
      "chunk_size": "16MB",
      "read_ahead_size": "64MB",
      "uid": 1000,
      "gid": 1000,
      "allow_other": true
    },
    "external_rclone": {}
  },
  "allowed_file_types": [
    "3gp", "ac3", "aiff", "alac", "amr", "ape", "asf", "asx", "avc", "avi", "bin", "bivx", "dat", "divx", "dts", "dv", "dvr-ms", "flac", "fli", "flv", "ifo", "m2ts", "m2v", "m3u", "m4a", "m4p", "m4v", "mid", "midi", "mk3d", "mka", "mkv", "mov", "mp2", "mp3", "mp4", "mpa", "mpeg", "mpg", "nrg", "nsv", "nuv", "ogg", "ogm", "ogv", "pva", "qt", "ra", "rm", "rmvb", "strm", "svq3", "ts", "ty", "viv", "vob", "voc", "vp3", "wav", "webm", "wma", "wmv", "wpl", "wtv", "wv", "xvid"
  ],
  "remove_stalled_after": "10m",
  "notifications": {},
  "download_folder": "/downloads",
  "refresh_interval": "30s",
  "max_downloads": 10,
  "categories": [
    "sonarr",
    "radarr"
  ],
  "folder_naming": "original_no_ext",
  "default_download_action": "symlink",
  "retries": 3
}
